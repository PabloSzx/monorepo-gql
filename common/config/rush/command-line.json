{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/command-line.schema.json",
  "commands": [
    {
      "name": "prettier",
      "commandKind": "global",
      "summary": "Used by the pre-commit Git hook. This command invokes Prettier to reformat staged changes.",
      "autoinstallerName": "rush-prettier",
      "shellCommand": "pretty-quick --staged"
    },
    {
      "commandKind": "global",
      "name": "test",
      "summary": "test",
      "autoinstallerName": "cli",
      "shellCommand": "dotenv -- pnpm test --recursive --no-sort --no-bail"
    },
    {
      "commandKind": "global",
      "name": "start",
      "summary": "start",
      "autoinstallerName": "cli",
      "shellCommand": "cross-env NODE_ENV=production dotenv -- pnpm start --recursive --no-sort --no-bail --stream"
    },
    {
      "commandKind": "global",
      "name": "dev",
      "summary": "dev",
      "autoinstallerName": "cli",
      "shellCommand": "cross-env-shell NODE_ENV=development \"pnpm build --recursive --filter graphql-gen --filter common && dotenv -- pnpm dev --recursive --no-bail --no-sort --stream\""
    },
    {
      "commandKind": "global",
      "name": "upd",
      "summary": "update dependencies",
      "autoinstallerName": "cli",
      "shellCommand": "upd"
    }
  ]
}